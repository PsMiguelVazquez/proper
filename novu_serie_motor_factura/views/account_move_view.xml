<?xml version="1.0" encoding="UTF-8"?>
<odoo>   
    <data>
        <record id="custom_account_move_view_form" model="ir.ui.view">
            <field name="name">	account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form" />
            <field name="mode">extension</field>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='other_info']" position="after">
                    <page name="numero_serie_motor" string="NÃºmero de Serie y Motor">
                        <field name="serie_motor">
                            <tree create="false" delete="true" editable="bottom">
                                <field name="product_id" readonly="1"/>
                                <field name="serial_number"/>
                                <field name="engine_number"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
                    
            </field>
        </record>
    </data>
</odoo>